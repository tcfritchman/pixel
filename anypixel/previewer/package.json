{
  "name": "anypixel-emulator",
  "description": "In-browser emulator for the Anypixel buttonwall",
  "version": "0.0.1",
  "main": "./src/index.js",
  "bin": {
    "preview": "./bin/preview"
  },
  "scripts": {
    "build": "browserify ./src/index.js > ../../public/js/anypixel/preview.js",
    "build-debug": "browserify ./src/index.js -d > ../../public/js/anypixel/preview.js"
  },
  "dependencies": {
    "anypixel": "^1.0.0",
    "browser-sync": "^2.11.1",
    "cli": "^0.11.1",
    "fs-extra": "^0.26.7"
  },
  "preferGlobal": true
}
